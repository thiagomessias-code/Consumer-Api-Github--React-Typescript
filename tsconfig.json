{
  "compilerOptions": {
 
    "lib": ["DOM", "DOM.Iterable", "ESNext"]
    ,                                       
     "jsx": "react-jsx",                                
     "moduleResolution": "node",                      
     "resolveJsonModule": true,                        
     "allowJs": true,                                  
     "noEmit": true,                                  
     "isolatedModules": true,                          
    "allowSyntheticDefaultImports": true,            
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                      
    "skipLibCheck": true                                 
  },
  "include": ["src"]
}
